  "support": {
    "source": "https://github.com/wdmg/yii2-settings",
    "issues": "https://github.com/wdmg/yii2-settings/issues"
  },
  "authors": [
    {
      "name": "Alexsander Vyshnyvetskyy",
      "email": "alex.vyshnyvetskyy@gmail.com"
    },
    {
      "name": "W.D.M.Group, Ukraine",
      "email": "wdmg.com.ua@gmail.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "^2.0.13"
  },
  "minimum-stability": "dev",
  "autoload": {
